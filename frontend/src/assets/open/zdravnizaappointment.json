{
  "program": "open",
  "resource": "appointment",
  "type": "appointment",
  "title": "Appointment",
  "api": "/api/appointment/",
  "successMessage": "Appointment successfully made",
  "style": "max-width: 568px;margin-left: auto;margin-right: auto;",
  "fields": [
    {
      "type": "appointmentdate",
      "name": "appointment_date",
      "text": "Select appointment date",
      "value": "date",
      "required": true
    },
    {
      "type": "appointmentselect",
      "name": "appointment_select",
      "text": "Select appointment time",
      "api": "/api/appointment/",
      "value": "time",
      "required": true
    },
    {
      "type": "text",
      "name": "appointment_first_name",
      "text": "First name",
      "value": "first_name",
      "maxlength": 80,
      "required": true
    },
    {
      "type": "text",
      "name": "appointment_last_name",
      "text": "Last name",
      "value": "last_name",
      "maxlength": 80,
      "required": false
    },
    {
      "type": "text",
      "name": "appointment_diagnos",
      "text": "Diagnos",
      "value": "comment",
      "maxlength": 255,
      "required": false
    },
    {
      "type": "text",
      "name": "appointment_email",
      "text": "Email",
      "value": "email",
      "maxlength": 80,
      "required": true
    },
    {
      "type": "text",
      "name": "appointment_mobile",
      "text": "Mobile",
      "value": "mobile",
      "maxlength": 20,
      "required": true
    },
    {
      "type": "actions",
      "fields": [
        {
          "type": "button",
          "name": "appointment_button",
          "text": "Submit",
          "action": "appointmentsave",
          "color": "success"
        }
      ]
    }
  ]
}
